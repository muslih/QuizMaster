<!DOCTYPE html>
<html>
  <head>
    <title>QuizMaster</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  	<%= render 'shared/header' %>
		<div class="container">
	    <div class="col-sm-12">
	      <%= render 'shared/flash_message',message: @model  %>
	      <%= yield %>
	      <footer>
	      	<hr>
	      	&copy; 2017 QuizMaster. Hand crafted with <strong>Ruby on Rails</strong>
	      </footer>
	    </div>
		</div>
 
  </body>
</html>
